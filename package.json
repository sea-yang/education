{
  "name": "education",
  "version": "0.1.0",
  "description": "education project, 2016",
  "main": "app.js",
  "author": "sea.yang",
  "license": "ISC",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start:dev\" \"npm run start:hot\"",
    "start:dev": "better-npm-run start:dev",
    "start:dev:pm2": "better-npm-run start:dev:pm2",
    "start:prod": "better-npm-run start:prod",
    "start:prod:pm2": "better-npm-run start:prod:pm2",
    "start:hot": "better-npm-run start:hot",
    "lint": "eslint -c .eslintrc .",
    "build": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod"
  },
  "betterScripts": {
    "start:dev": {
      "command": "node ./bin/server",
      "env": {
        "NODE_ENV": "development",
        "PORT": "3000"
      }
    },
    "start:dev:pm2": {
      "command": "pm2 start ./bin/server.js --name education",
      "env": {
        "NODE_ENV": "development",
        "PORT": "3000"
      }
    },
    "start:hot": {
      "command": "node ./webpack/hot-server",
      "env": {
        "NODE_ENV": "development",
        "UV_THREADPOOL_SIZE": "100",
        "PORT": "3000"
      }
    },
    "start:prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": "3000"
      }
    },
    "start:prod:pm2": {
      "command": "pm2 start ./bin/server.js -i 0 --name education",
      "env": {
        "NODE_ENV": "production",
        "PORT": "3000"
      }
    },
    "build:dev": {
      "command": "webpack --verbose --colors --progress --display-error-details --config webpack/build",
      "env": {
        "NODE_ENV": "production",
        "HOST": "w.jenniferstudio.cn",
        "PORT": "3000",
        "PUBLIC_PATH": "dist"
      }
    },
    "build:prod": {
      "command": "webpack --verbose --colors --progress --display-error-details --config webpack/build",
      "env": {
        "NODE_ENV": "production",
        "HOST": "st.siline.cn",
        "PORT": "80",
        "PUBLIC_PATH": "static/education"
      }
    }
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.1",
    "crypto": "0.0.3",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "hbs": "^4.0.0",
    "helmet": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.0",
    "material-ui": "^0.15.0",
    "morgan": "^1.7.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-helmet": "^3.0.1",
    "react-lazyload": "^2.0.0",
    "react-redux": "^4.4.2",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.4",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.4.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "request": "^2.72.0",
    "serve-favicon": "^2.3.0",
    "xml2json": "^0.9.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "better-npm-run": "^0.0.8",
    "clean-webpack-plugin": "^0.1.8",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "glob": "^7.0.3",
    "image-webpack-loader": "^1.6.3",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "react-transform-hmr": "^1.0.4",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.9",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-isomorphic-tools": "^2.2.44",
    "webpack-md5-hash": "^0.0.5"
  }
}
